<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.linchuanedu.edu.eduapp.view.RegisterActivity">

        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView"
                tools:layout_constraintTop_creator="1" tools:layout_constraintRight_creator="1"
                app:layout_constraintRight_toRightOf="@+id/textView2" android:layout_marginTop="5dp"
                tools:layout_constraintLeft_creator="1" app:layout_constraintLeft_toLeftOf="@+id/textView2"
                app:layout_constraintTop_toTopOf="@+id/editText"/>
        <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView2"
                tools:layout_constraintBottom_creator="1" android:layout_marginStart="55dp"
                app:layout_constraintBottom_toBottomOf="@+id/editText2" tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="15dp" app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="55dp"/>
        <Button
                android:text="Button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button"
                app:layout_constraintRight_toLeftOf="@+id/editText2" tools:layout_constraintTop_creator="1"
                tools:layout_constraintRight_creator="1" android:layout_marginEnd="10dp" android:layout_marginTop="41dp"
                app:layout_constraintTop_toBottomOf="@+id/editText2" android:layout_marginRight="10dp"/>
        <Button
                android:text="Button"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/button3"
                tools:layout_constraintTop_creator="1" tools:layout_constraintRight_creator="1"
                tools:layout_constraintBottom_creator="1" app:layout_constraintBottom_toBottomOf="@+id/button"
                android:layout_marginEnd="60dp" app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="@+id/button" android:layout_marginRight="60dp"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="phone"
                android:ems="10"
                android:id="@+id/editText"
                tools:layout_constraintBottom_creator="1" app:layout_constraintBottom_toTopOf="@+id/editText2"
                android:layout_marginStart="1dp" tools:layout_constraintLeft_creator="1"
                android:layout_marginBottom="19dp" app:layout_constraintLeft_toLeftOf="@+id/editText2"
                android:layout_marginLeft="1dp"/>
        <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:inputType="textPassword"
                android:ems="10"
                android:id="@+id/editText2"
                tools:layout_constraintTop_creator="1" android:layout_marginStart="49dp"
                android:layout_marginTop="240dp" tools:layout_constraintLeft_creator="1"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintLeft_toRightOf="@+id/textView2"
                android:layout_marginLeft="49dp"/>
</android.support.constraint.ConstraintLayout>
